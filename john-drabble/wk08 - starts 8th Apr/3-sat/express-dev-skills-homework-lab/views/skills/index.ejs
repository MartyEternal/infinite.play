<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <title>Coder Skills</title>
  </head>
  <body>
    <div class="container">
      <%- include('./partials/heading') %>
      <ul>
        <% skills.forEach(function(s) { %>
        <li>
          <a href="/skills/show/<%= s.id %>">
            Click for details on <%= s.name%>
          </a>
        </li>
        <% }); %>
      </ul>
    </div>
  </body>
</html>
