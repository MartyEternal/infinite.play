<%- include('../partials/header') %>

  

    <div class = "flex-container">
      <% skills.forEach(function(s) { %>
        <div class = "links">
          <a href="/skills/<%= s.id %>">
            <%= s.skill %>
            <% if (s.imgUrl) { %>
            <img src="<%= s.imgUrl %>" alt="<%= s.skill %>">
            <% } else { %>
            <img src="https://t3.ftcdn.net/jpg/02/68/55/60/360_F_268556012_c1WBaKFN5rjRxR2eyV33znK4qnYeKZjm.jpg" alt="<%= s.skill %>">
            <% } %>
          </a>
        </div>
        <%})%>
    </div>
  </div class = "add"> <a href = "/skills/new">Add Skill</a></div>
  

<%- include('../partials/footer') %>
  </body>
</html>
