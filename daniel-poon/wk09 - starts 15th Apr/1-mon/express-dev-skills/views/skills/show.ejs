<%- include('../partials/header') %>


    <h3><%= skill.skill %></h3>
    <% if (skill.imgUrl) { %>
    <img src="<%= skill.imgUrl %>" style="width: 100px; height: auto;" />
    <% } else { %>
        <img src="https://t3.ftcdn.net/jpg/02/68/55/60/360_F_268556012_c1WBaKFN5rjRxR2eyV33znK4qnYeKZjm.jpg" style="width: 100px; height: auto;" />
    <% } %>
    <h8><%= skill.description %></h8>
    
          <form action="/skills/<%= skill.id %>?_method=DELETE"
            class="delete-form" method="POST">
            <button type="submit">Delete Skill</button>
          </form>

          <a href="/skills/edit/<%= skill.id %>">Edit Skill</a>
      
</body>
</html>
<%- include('../partials/footer') %>

